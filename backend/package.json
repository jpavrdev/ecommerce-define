{
  "name": "ecommercedefine-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "lint": "echo 'no linter configured'",
    "db:migrate": "sequelize db:migrate --config ./config/config.cjs --env development",
    "db:migrate:undo": "sequelize db:migrate:undo --config ./config/config.cjs --env development",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all --config ./config/config.cjs --env development",
    "db:seed": "sequelize db:seed --config ./config/config.cjs --env development",
    "db:seed:all": "sequelize db:seed:all --config ./config/config.cjs --env development",
    "db:seed:undo:all": "sequelize db:seed:undo:all --config ./config/config.cjs --env development",
    "test:unit": "npm run build && node tests/run-all.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.18.13",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
